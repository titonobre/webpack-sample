{
  "name": "webpack-sample",
  "version": "0.0.0",
  "description": "Webpack Sample",
  "scripts": {
    "build": "webpack --config config/webpack.prod.js",
    "check": "npm run lint && npm run test",
    "clean": "rimraf dist reports",
    "lint": "npm run lint-styles && npm run lint-src && npm run lint-test",
    "lint-src": "eslint src/**/*.js",
    "lint-styles": "sass-lint src/**/*.scss -v -q",
    "lint-test": "eslint test/** --global inject,before --env node",
    "start": "nodemon --watch config --exec \"webpack-dev-server --config config/webpack.dev.js --hot --inline --port 8181\"",
    "test": "karma start karma.conf.js --single-run",
    "test-watch": "karma start karma.conf.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "angular": "1.6.4",
    "babel-polyfill": "6.23.0"
  },
