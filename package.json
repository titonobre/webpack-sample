    "awesome-typescript-loader": "3.1.3",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "css-loader": "0.28.1",
    "eslint": "3.19.0",
